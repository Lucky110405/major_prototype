# File: /collaborative-multi-modal-agentic-framework/collaborative-multi-modal-agentic-framework/agents/collaborative/tests.py
def test_collaborative_agent():
    agent1 = CollaborativeAgent(agent_id="agent_1")
    agent2 = CollaborativeAgent(agent_id="agent_2")
    
    agent1.update_state({"task": "data_analysis"})
    agent2.update_state({"task": "data_collection"})
    
    assert agent1.state["task"] == "data_analysis"
    assert agent2.state["task"] == "data_collection"